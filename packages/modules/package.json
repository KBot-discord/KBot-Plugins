{
  "name": "@kbotdev/plugin-modules",
  "version": "0.1.2",
  "description": "Module plugin for the Sapphire Framework",
  "author": "Killbasa",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/kbotdev/kbot-plugins.git",
    "directory": "packages/modules"
  },
  "homepage": "https://gitlab.com/kbotdev/kbot-plugins/-/tree/main/packages/modules",
  "dependencies": {
    "@sapphire/framework": "^3.1.4",
    "discord.js": "^13.12.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.14.2",
    "gen-esm-wrapper": "^1.1.3",
    "typedoc": "^0.23.22",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.x"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*",
    "./register.*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./register": {
      "import": "./register.mjs",
      "require": "./register.js",
      "types": "./register.d.ts"
    }
  },
  "sideEffects": [
    "./dist/register.js",
    "./register.js",
    "./register.mjs"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .ts src",
    "format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,json}\"",
    "build": "tsc -b src && yarn esm:register && yarn esm:default",
    "esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
    "esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "docs": "typedoc --entryPointStrategy packages --out ../../docs/modules ."
  },
  "packageManager": "yarn@3.2.4"
}
